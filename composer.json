{
  "name": "devvoh/phorage",
  "type": "library",
  "description": "Phorage is a lightweight, naive file-based database",
  "keywords": ["library", "micro", "database"],
  "homepage": "https://github.com/devvoh/Phorage",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin de Graaf",
      "email": "hello@devvoh.com",
      "homepage": "https://devvoh.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.11",
    "friendsofphp/php-cs-fixer": "^3.59"
  },
  "autoload": {
    "psr-4": {
      "Devvoh\\Phorage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devvoh\\Phorage\\Tests\\": "tests"
    }
  }
}
